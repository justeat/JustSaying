{
  "version": "5.0.0",
  "authors": [ "JUST EAT plc" ],
  "copyright": "JUST EAT plc",
  "title": "JustSaying",
  "description": "A light-weight message bus on top of AWS SNS and SQS",
  "buildOptions": {
    "warningsAsErrors": true,
    "platform": "anycpu",
    "languageVersion": "csharp6",
    "debugType": "portable"
  },
  "packOptions": {
    "owners": [ "JUST EAT plc" ],
    "summary": "SNS and SQS Messaging",
    "tags": [ "aws", "sns", "sqs" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/justeat/JustSaying.git"
    },
    "requireLicenseAcceptance": true,
    "licenseUrl": "https://github.com/justeat/JustSaying/blob/develop/LICENSE.md",
    "projectUrl": "https://github.com/justeat/JustSaying"
  },
  "configurations": {
    "Debug": {

    },
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },
  "dependencies": {
    "AWSSDK.Core": "3.2.8-rc",
    "AWSSDK.SimpleNotificationService": "3.2.8-rc",
    "AWSSDK.SQS": "3.2.8-rc",
    "JustBehave": "1.0.0.12",
    "JustSaying": "5.0.0",
    "JustSaying.AwsTools": "5.0.0",
    "JustSaying.Messaging": "5.0.0",
    "JustSaying.Models": "5.0.0",
    "JustSaying.TestingFramework": "5.0.0",
    "Newtonsoft.Json": "9.0.1",
    "NLog": "4.4.0-betaV15",
    "NSubstitute": "1.10.0",
    "NUnit": "3.4.1",
    "StructureMap": "4.4.0",
    "dotnet-test-nunit": "3.4.0-beta-2"
  },
  "testRunner": "nunit",
  "frameworks": {
    "net46": {
      "dependencies": {}
    }
  }
}
